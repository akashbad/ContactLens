.row-fluid
  .offset1.span10#approve-container.thick-shadow
    .row.tags-row
      .tags-container.span4
        %input#tags-input{:type=>"text", :placeholder=>"Tags", autocomplete:"off", autofocus: true}
      .span4
        %ul.nav.nav-pills#tags
      #buttons-container.span4
        %button.btn.btn-success#approve{:type=>"button"} Approve
        %button.btn.btn-warning#later{:type=>"button"} Later
        %button.btn.btn-danger#reject{:type=>"button"} Reject
    .row#info-container
      .span2#prof-pic-container
        %img#prof-pic
      .span3
        %p
          %h3 Delian Asparouhov
          %h4 MIT Class of 2015
          %h4 Worked at Square
      .span3#social-info
        %p
          :plain
            <a href="https://twitter.com/mitdelian" class="twitter-follow-button" data-show-count="true" data-lang="en">Follow @MITDelian</a>
          :plain
            <div class="fb-like" data-href="http://contactlens.herokuapp.com/approve" data-send="false" data-layout="button_count" data-width="450" data-show-faces="false"></div>
          %br
          :plain
            <script type="IN/Share" data-counter="right"></script>
      .span2
        %p
          %h4 Location 
          Salt Lake City
      .span2#location-container
        :plain
          <img src="http://maps.googleapis.com/maps/api/staticmap?center=40.75,-111.8833&zoom=11&size=128x128&sensor=false">
    #cards-container.row
      .span12
        .row-fluid
          .span3
            .card.thin-shadow
              .blue-bar
              / %img.twitter-pic
              .inner-card
                %img.history-icon.card-icon{:alt=>"Twitter", :src=>"/assets/twitter.png"}
                %h5.handle @MITDelian
                %h6.city Boston, MA
                %p.small-text.bio Sophomore at MIT. Co-founder of @nightingaletalk. Coder and Creator.
          .span5
            .card.thin-shadow
              .blue-bar
              .inner-card
                %img.history-icon.card-icon{:alt=>"Facebook", :src=>"/assets/facebook.png"}
                %h5.handle Delian Tihomirov Asparouhov
                %h6.city Boston, MA
                %p.small-text.bio 
                  Eliza-"I've been really happy lots of times, but I never shaved my hair"
                  %br
                  Victor- "Shouldn't natural selection have taken care of Delian by now?
                  %br
                  Mrs. Slabaugh-"Don't worry Victor, Delian will die before he can reproduce"
          .span4
            .card.thin-shadow
              .blue-bar
              .inner-card
                %img.history-icon.card-icon{:alt=>"LinkedIn", :src=>"/assets/linkedin.png"}
                %h5.handle Delian Asparouhov
                %h6.city Boston, MA
                %p.small-text.bio Sophomore at MIT. Computer Science Major from Utah.
        .row-fluid
          .span5
            .card.thin-shadow
              .blue-bar
              .inner-card
                %img.history-icon.card-icon{:alt=>"LinkedIn", :src=>"/assets/linkedin.png"}
                %h5.handle Education
                %h6.city MIT
                %p.small-text.bio Class of 2015 Computer Science and Electrical Engineering
                %h6.city West High School
                %p.small-text.bio Class of 2011

